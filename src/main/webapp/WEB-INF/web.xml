<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
    <display-name>SCM Federator</display-name>

    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>

    <listener>
        <listener-class>com.surevine.gateway.scm.FederateSCMContextListener</listener-class>
    </listener>

    <listener>
        <listener-class>com.surevine.gateway.scm.IncomingFileContextListener</listener-class>
    </listener>
</web-app>
